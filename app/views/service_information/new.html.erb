<% provide(:title, @title ) %>
<% provide(:active, 'services' ) %>

<% content_for :body do %>

  <% if !@serviceInformation.errors.empty? %>
    <%= error_messages("Warning!", "There have been some errors you need to fix.", @serviceInformation) %>
  <% end %>
  <%= form_for @serviceInformation, url: {controller: 'service_information', action: 'create'}, :html => { class: "form-signin", role: "form" } do  |form|  %>
    <%= render partial: 'form_fields', locals: {serviceInformation: @serviceInformation, form: form } %>
  <% end %>
   
<% end %>
