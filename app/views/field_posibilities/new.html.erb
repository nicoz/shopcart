<% provide(:title, @title ) %>
<% provide(:active, 'fieldTypes' ) %>

<% content_for :body do %>

  <% if !@field_posibility.errors.empty? %>
    <%= error_messages("Warning!", "There have been some errors you need to fix.", @field_posibility) %>
  <% end %>
  <%= form_for @field_posibility, url: {controller: 'field_posibilities', action: 'create'}, :html => { class: "form-signin", role: "form" } do  |form|  %>
    <%= render partial: 'form_fields', locals: {field_posibility: @field_posibility, form: form } %>
  <% end %>
   
<% end %>
