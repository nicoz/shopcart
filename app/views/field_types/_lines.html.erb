<%= will_paginate lines %>

<% if field_type.field_type == 'list' %>
  <%= render partial: 'lines_posibilities', locals: {field_type: field_type, lines: lines} %>
<% end %>

<% if field_type.field_type == 'map' %>
  <%= render partial: 'lines_map', locals: {field_type: field_type, lines: lines} %>
<% end %>

<%= will_paginate lines %>

<script>
  $("div.pagination ul").addClass('pagination');
  
  $('.previous_page > a').html("Previo");
  $('.next_page > a').html("Proximo");
</script>
