<div class="row">
  <div class="col-md-6">
    <h2>
      <%= item.name %>
    </h2>
  </div>
</div>

<div class="row">
  <div class="col-md-2">
    <%= link_to edit_item_path(item), class: "btn btn-lg btn-default" do %>
      <span class="glyphicon glyphicon-pencil"></span> Editar Datos
    <% end %>
  </div>
  <%= render partial: '/shared/back', locals: {path: items_path} %>
</div>


